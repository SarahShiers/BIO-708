# Create the timeline data
timeline_data <- data.frame(
year = c(1490, 1826, 1877, 1914, 1918, 1929, 1944, 1945, 1966, 1967, 1970, 1971, 1976, 1980, 1991, 1997, 2000, 2017, 2020, 2021, 2022),
event = c("Da Vinci describes sound in water", "First measurement of sound speed in water", "Theory of Sound published", "Early sonar developed", "WWI: Acoustics in warfare", "ASA founded", "SOFAR channel discovered", "Post-WWII research shift", "Sea Grant Program established", "First acoustics textbook", "NOAA established", "Ocean Exploration Decade", "Acoustics textbooks widely available", "Cold War growth", "End of Cold War", "Lackie Report", "PAM emerges", "Task Force Ocean launched", "Ocean sound as Essential Variable", "UN Ocean Science Decade", "ONR education study")
)
# Create the plot
p <- ggplot(timeline_data, aes(x = year, y = 1, label = event)) +
geom_point(color = "dodgerblue3", size = 4) +
geom_segment(aes(x = year, xend = year, y = 0.9, yend = 1.1), color = "dodgerblue3") +
geom_text(aes(y = 1.2), angle = 45, hjust = 0, size = 3) +
scale_x_continuous(breaks = seq(1490, 2030, by = 50)) +
theme_minimal() +
theme(
axis.text.y = element_blank(),
axis.title.y = element_blank(),
panel.grid.major.y = element_blank(),
panel.grid.minor.y = element_blank(),
plot.title = element_text(hjust = 0.5, size = 16, face = "bold"),
plot.subtitle = element_text(hjust = 0.5, size = 12, face = "italic")
) +
labs(
title = "Ocean Acoustics Timeline",
subtitle = "Key events in the history of ocean acoustics",
x = "Year",
y = NULL
)
# Display the plot
print(p)
